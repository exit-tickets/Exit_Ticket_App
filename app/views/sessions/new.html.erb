<h1> General Assembly </h1>
<h2> Exit Ticket App </h2>

<%= form_tag "/login" do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <br>
  <%= submit_tag "Log In" %>
<% end %>

